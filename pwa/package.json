{
  "name": "weleda-webcenter-text-export",
  "version": "1.0.0",
  "description": "Converts Web Center's XML export file into raw text",
  "keywords": [
    "react",
    "app",
    "weleda"
  ],
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/D3strukt0r/weleda-webcenter-text-export/issues",
    "email": "contact@d3strukt0r.me"
  },
  "license": "GPL-3.0-only",
  "author": {
    "name": "D3strukt0r",
    "email": "dev@d3strukt0r.me"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^4.12",
    "@material-ui/icons": "^4.11",
    "@testing-library/jest-dom": "^5.11",
    "@testing-library/react": "^11.1",
    "@testing-library/user-event": "^12.1",
    "@types/jest": "^26.0",
    "@types/node": "^12.0",
    "@types/react": "^17.0",
    "@types/react-dom": "^17.0",
    "@types/testing-library__jest-dom": "^5.14",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^7.0",
    "lint-staged": "^11.2",
    "prettier": "^2.4",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-dropzone": "^11.4",
    "react-scripts": "4.0",
    "source-map-explorer": "^2.5.2",
    "typeface-roboto": "^1.1",
    "typescript": "^4.1",
    "web-vitals": "^1.0",
    "workbox-background-sync": "^5.1",
    "workbox-broadcast-update": "^5.1",
    "workbox-cacheable-response": "^5.1",
    "workbox-core": "^5.1",
    "workbox-expiration": "^5.1",
    "workbox-google-analytics": "^5.1",
    "workbox-navigation-preload": "^5.1",
    "workbox-precaching": "^5.1",
    "workbox-range-requests": "^5.1",
    "workbox-routing": "^5.1",
    "workbox-strategies": "^5.1",
    "workbox-streams": "^5.1"
  },
  "devDependencies": {
    "gh-pages": "^3.2"
  },
  "private": true,
  "dependenciesMeta": {
    "webpack-dev-server": {
      "unplugged": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.0.2"
}
